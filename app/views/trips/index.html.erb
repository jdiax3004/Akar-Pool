<p id="notice"><%= notice %></p>

<h1>Trips</h1>

<table>
  <thead>
    <tr>
      <th>Names</th>
      <th>Seats</th>
      <th>Car</th>
      <th>Departure</th>
      <th>Placemarks</th>
      <th>—no-stylesheets</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @trips.each do |trip| %>
      <tr>
        <td><%= trip.names %></td>
        <td><%= trip.seats %></td>
        <td><%= trip.car %></td>
        <td><%= trip.departure %></td>
        <td><%= trip.PlaceMarks %></td>
        <td><%= trip.—no-stylesheets %></td>
        <td><%= link_to 'Show', trip %></td>
        <td><%= link_to 'Edit', edit_trip_path(trip) %></td>
        <td><%= link_to 'Destroy', trip, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Trip', new_trip_path %>
